{
  "name": "搜素跳转",
  "version": "1.0",
  "description": "换个方式搜索试试",
  "manifest_version": 2,
  "icons": {
    "128": "search-switch-128.png"
  },
  "browser_action": {
    "default_icon": "search-switch-128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", 
    "*://www.baidu.com/*",
    "*://www.google.com.hk/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [{
    "matches": [
      "*://*.baidu.com/*",
      "*://*.google.com/*",
      "*://www.google.com.hk/*"
    ],
    "js":["jquery-2.0.0.min.js", "content_script.js"]
  }]
}
